<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="limp_contract_form_add_no_quality" model="ir.ui.view">
        <field name="name">limp.contract.form.add_no_quality</field>
        <field name="model">limp.contract</field>
        <field name="inherit_id" ref="limp_contract.limp_contract_form"/>
        <field name="arch" type="xml">
            <field name="periodicity" position="after">
                <field name="no_quality"/>
            </field>
        </field>
    </record>

    <record id="limp_contract_search_add_no_quality" model="ir.ui.view">
        <field name="name">limp.contract.search.add_no_quality</field>
        <field name="model">limp.contract</field>
        <field name="inherit_id" ref="limp_contract.limp_contract_search"/>
        <field name="arch" type="xml">
            <field name="upamount_date" position="after">
                <field name="no_quality"/>
            </field>
        </field>
    </record>
</odoo>
