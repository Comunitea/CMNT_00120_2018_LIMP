<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">

        <record id="seq_stock_service_picking" model="ir.sequence">
            <field name="name">Service pickings</field>
            <field name="code">stock.service.picking</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="prefix">C-</field>
        </record>

        <record id="seq_service_picking_maintenance" model="ir.sequence">
            <field name="name">Maitenance service pickings</field>
