            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="prefix">M-</field>
        </record>

        <record id="seq_admission_no" model="ir.sequence">
            <field name="name">Admission no.</field>
            <field name="code">waste.admission.number</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="prefix">DAR</field>
        </record>

</odoo>
