<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

        <record id="seq_treatment_certificate_ddd" model="ir.sequence">
            <field name="name">Certificate DDD</field>
            <field name="code">treatment.certificate.ddd</field>
            <field name="prefix">CTDDD</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_treatment_certificate_legionella" model="ir.sequence">
            <field name="name">Certificate Legionella</field>
            <field name="code">treatment.certificate.legionella</field>
            <field name="prefix">CTL</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_legionella_sample" model="ir.sequence">
            <field name="name">Legionella Sample Seq.</field>
            <field name="code">legionella.sample.seq</field>
            <field name="prefix">AN</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

</odoo>
