        <record id="view_partner_bank_iban_form_inherit" model="ir.ui.view">
            <field name="name">res.partner.bank.form.iban.inherit</field>
            <field name="model">res.partner.bank</field>
            <field name="inherit_id" ref="base_iban.view_partner_bank_iban_form"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <field name="iban" position="replace">
                    <field name="iban" on_change="onchange_iban(iban)"/>
                </field>
            </field>
          </record>

        <record id="view_partner_iban_form_imp" model="ir.ui.view">
            <field name="name">res.partner.form.iban.inherit.imp</field>
