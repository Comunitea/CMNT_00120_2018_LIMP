<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <delete model="ir.ui.menu" id="purchase.menu_purchase_rfq"/>

    <delete model="ir.values" search="[('key','=','default'), ('model', '=', 'purchase.order')]"/>
    <record id="limpergal_purchase_order_form" model="ir.ui.view">
        <field name="name">limpergal.purchase.order.form</field>
        <field name="model">purchase.order</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <field name="warehouse_id" position="replace">
                <field name="warehouse_id" required="1" on_change="onchange_warehouse_id(warehouse_id)" widget="selection"/>
            </field>
        </field>
    </record>
</data>
</openerp>
