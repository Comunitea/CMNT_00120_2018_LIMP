<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_picking_out_add_operator" model="ir.ui.view">
        <field name="name">stock.picking.out.form.add_operator</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="limp_service_picking.view_picking_out_form_rep_buttons"/>
        <field name="arch" type="xml">
            <field name="manager_or_productor" position="after">
                <field name="operator_partner_id"/>
                <field name="nt_doc_id" domain="[('operator_partner_id', '=', operator_partner_id),('manager_partner_id', '=', partner_id)]"/>
                <field name="dcs_no"/>
            </field>
            <field name="state" position="before">
                <button name="check_DI_data" type="object" string="Check data"/>
            </field>
        </field>
    </record>

</odoo>
