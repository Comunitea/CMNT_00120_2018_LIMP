<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record id="analytic_incidence_set_end_date_form" model="ir.ui.view">
            <field name="name">analytic.incidence.set.end.date.form</field>
            <field name="model">analytic.incidence.set.end.date</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Set end date">
                    <field name="end_date"/>
                    <group colspan="4">
                        <button icon="gtk-cancel" special="cancel" string="Cancel"/>
                        <button icon="gtk-ok" name="set_end_date" string="Set" type="object"/>
                    </group>
                </form>
            </field>
        </record>

        <act_window
            id="action_analytic_incidence_set_end_date" name="Replace employee"
            res_model="analytic.incidence.set.end.date"
            src_model="remuneration"
            view_type="form"
            view_mode="form"
            target="new"
            view_id="analytic_incidence_set_end_date_form"
            key2="client_action_multi"
            />

    </data>
</openerp>
