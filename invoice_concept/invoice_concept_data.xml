<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record forcecreate="True" id="ir_cron_weekly_concept_invoice" model="ir.cron">
                <field name="name">Run concept invoices</field>
                <field eval="False" name="active" />
                <field name="user_id" ref="base.user_root" />
                <field name="interval_number">1</field>
                <field name="interval_type">months</field>
                <field name="nextcall" eval="(time.localtime()[0] % 4 == 0 and time.localtime()[0] % 100 != 0 or time.localtime()[0] % 400 == 0) and time.strftime('%Y-%m-' + str([31,29,31,30,31,30,31,31,30,31,30,31][time.localtime()[1]-1])+' 22:00') or time.strftime('%Y-%m-' + str([31,28,31,30,31,30,31,31,30,31,30,31][time.localtime()[1]-1])+' 22:00')"/>
                <field name="numbercall">-1</field>
                <field eval="False" name="doall" />
                <field eval="'account.analytic.account'" name="model" />
                <field eval="'run_invoice'" name="function" />
                <field eval="'(False,)'" name="args" />
        </record>

    </data>
</openerp>
